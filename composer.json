{
    "name": "wulfheart/laravel-fileloquent",
    "description": "",
    "keywords": [
        "wulfheart",
        "laravel-fileloquent"
    ],
    "homepage": "https://github.com/wulfheart/laravel-fileloquent",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Alex Wulf",
            "email": "alex.f.wulf@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Wulfheart\\LaravelFileloquent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wulfheart\\LaravelFileloquent\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wulfheart\\LaravelFileloquent\\LaravelFileloquentServiceProvider"
            ],
            "aliases": {
                "LaravelFileloquent": "Wulfheart\\LaravelFileloquent\\LaravelFileloquentFacade"
            }
        }
    }
}
